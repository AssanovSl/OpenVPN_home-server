dev tun

# Адреса туннеля (зеркально к серверу)
ifconfig 10.1.0.2 10.1.0.1

cipher AES-128-CBC

# Статический ключ (тот же, что у сервера)
<secret>
-----BEGIN OpenVPN Static key V1-----

-----END OpenVPN Static key V1-----
</secret>

# Адрес сервера (замени на свой белый IP или DDNS-имя)
remote <YOUR STATIC IP> 1194 udp

# Для отладки
verb 3

# Если нужно, чтобы клиент ходил в локалку сервера (192.168.1.0/24, например)
route 192.168.1.0 255.255.255.0
